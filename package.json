{
  "name": "sme-app",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/expo": "^26.0.3",
    "@types/jest": "^22.2.3",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-native": "^0.55.12",
    "@types/react-native-collapsible": "^0.11.0",
    "@types/react-native-star-rating": "^1.0.0",
    "@types/react-navigation": "^1.5.11",
    "@types/react-test-renderer": "^16.0.1",
    "@wli/react-native-modal-popover": "^0.0.7",
    "concurrently": "^3.5.1",
    "jest-expo": "25.0.0",
    "prettier-eslint": "^8.8.1",
    "pretty-quick": "^1.4.1",
    "react-native-scripts": "1.11.1",
    "react-test-renderer": "16.2.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "node node_modules/jest/bin/jest.js",
    "lint": "tslint src/**/*.ts",
    "tsc": "tsc",
    "clean": "rimraf build",
    "build": "yarn run clean && yarn run tsc --",
    "watch": "yarn run build -- -w",
    "watch-run-android": "concurrently \"yarn run watch\" \"yarn run android\"",
    "start-run-android": "concurrently \"yarn run build\" \"yarn run android\"",
    "build-run-android": "yarn run build && yarn run watch-run-android ",
    "watch-run-ios": "concurrently \"yarn run watch\" \"yarn run ios\"",
    "build-run-ios": "yarn run build && yarn run watch-run-ios ",
    "watch-start": "concurrently \"yarn run watch\" \"yarn run start\"",
    "build-start": "yarn run build && yarn run watch-Start ",
    "precommit": "pretty-quick --staged"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^6.3.1",
    "apollo-boost": "^0.1.7",
    "create-react-native-app": "^1.0.0",
    "expo": "^25.0.0",
    "global": "^4.3.2",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "native-base": "^2.4.3",
    "prop-types": "^15.6.1",
    "react": "16.2.0",
    "react-apollo": "^2.1.4",
    "react-native": "0.52.0",
    "react-native-collapsible": "^0.11.2",
    "react-native-modal-popover": "^0.0.10",
    "react-native-slider": "^0.11.0",
    "react-native-star-rating": "^1.0.9",
    "react-navigation": "^1.5.8",
    "tslint-config-prettier": "^1.12.0"
  }
}